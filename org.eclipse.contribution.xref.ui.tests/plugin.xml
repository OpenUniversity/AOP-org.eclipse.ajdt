<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="org.eclipse.contribution.xref.ui.test"
   name="%pluginName"
   version="92.2.29"
   provider-name="%providerName">

   <runtime>
      <library name="xrefuitest.jar">
         <export name="*"/>
      </library>
   </runtime>
   <requires>
      <import plugin="org.eclipse.contribution.xref.ui"/>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.jdt.core"/>
      <import plugin="org.junit"/>
      <import plugin="org.eclipse.contribution.xref.core"/>
   </requires>

   <extension
         point="org.eclipse.contribution.xref.core.providers">
      <xrefProvider
            label="Test XReference Provider Label"
            enabled="true"
            class="org.eclipse.contribution.xref.internal.ui.views.TestXRefProvider"
            id="org.eclipse.contribution.xref.core.test.testXRefProvider">
      </xrefProvider>
      <xrefProvider
            label="Test XReference Provider With Entities Label"
            enabled="true"
            class="org.eclipse.contribution.xref.internal.ui.views.TestXRefProviderWithEntities"
            id="org.eclipse.contribution.xref.core.test.testXRefProviderWithEntities">
      </xrefProvider>
   </extension>
</plugin>
